{
  "version": "1.0.0-*",
  "buildOptions": {
    "warningsAsErrors": true,
    "debugType": "portable",
    "xmlDoc": true,
    "embed": {
      "include": [
        "i18next/i18next-en.json",
        "i18next/i18next-fr.json",
        "i18next/i18next-en-CA.json",
        "i18next/i18next-en-US.json",
        "i18next/i18next-fr-CA.json"
      ]
    },
    "nowarn": [
      "SA1616",
      "SA1633",
      "SA1606",
      "SA1600",
      "CS1591",
      "SA1508",
      "SA1515",
      "SA1642",
      "SA1614",
      "SA1623",
      "SA1602"
    ]
  },
  "dependencies": {
    "MailKit": "1.16.0",
    "Newtonsoft.Json": "10.0.1",
    "System.Security.Claims": "4.3.0",
    "Microsoft.Extensions.Configuration": "1.1.0",
    "Microsoft.Extensions.Configuration.Binder": "1.1.0",
    "Microsoft.EntityFrameworkCore": "1.1.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "Data": {
      "target": "project"
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    }
  }
}
